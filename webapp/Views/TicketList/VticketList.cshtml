                                    /* proceso fallido catch listado datos*/
                                    $("#GrillaLstArchivoTicket").addClass("hidden");
                                    $("#imgCargaLstArchivoTicket").addClass("hidden");
                                    $("#msjErrorLstArchivoTicket").addClass("hidden");
